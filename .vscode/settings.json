                "settings": {
                    "foreground": "#FFFFFF"
                }
            },
            {
                "scope": [
                    "support.class",
                    "keyword.control.at-rule.media.css",
                    "meta.at-rule.media.header.css"
                ],
                "settings": {
                    "foreground": "#16c5bc"
                }
            },
            {
                "scope": [
                    "comment.line.double-slash.js"
                ],
                "settings": {
                    "foreground": "#6D7485"
                }
            },
            {
                "scope": [
                    "keyword.operator.expression.typeof.js"
                ],
                "settings": {
                    "foreground": "#399EF4"
                }
            },
            {
                "scope": [
                    "entity.name.function.js"
                ],
                "settings": {
                    "foreground": "#DCDCAA"
                }
            },
            {
                "scope": [
                    "new.expr.js",
                    "meta.var.expr.js"
                ],
                "settings": {
                    "foreground": "#16c5bc"
                }
            },
            {
                "scope": [
                    "string.quoted.double.js",
                    "string.quoted.single.js"
                ],
                "settings": {
                    "foreground": "#CE9178"
                }
            },
            {
                "scope": [
                    "punctuation.accessor.optional.js",
                    "meta.var.expr.js",
                    "meta.function-call.js"
                ],
                "settings": {
                    "foreground": "#FFFFFF"
                }
            },
            {
                "scope": "keyword.control.bg3txt",
                "settings": {
                    "foreground": "#C586C0"
                }
            },
            {
                "scope": "keyword.operator.bg3txt",
                "settings": {
                    "foreground": "#ffa200"
                }
            },
            {
                "scope": "string.first-column.bg3txt",
                "settings": {
                    "foreground": "#9CDCFE"
                }
            }
        ]
